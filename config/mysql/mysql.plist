<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>mysql</string>
        <key>ProgramArguments</key>
        <array>
            <string>/opt/mysql/bin/mysqld_safe</string>
            <string>--defaults-file=/usr/local/etc/mysql/my.cnf</string>
            <string>--ledir=/opt/mysql/bin</string>
        </array>
        <key>KeepAlive</key>
        <false/>
        <key>RunAtLoad</key>
        <true/>
    </dict>
</plist>
